<table class ="profile" summary= "Profile Information"> 
  <tr>
    <td class = "main">
      <h1><%= @user.name %> </h1>
    </td> 

    <td class ="sidebar round">
      <strong>Name</strong> <%=@user.name %> <%=@user.lastname %> <br /> 
      <strong>URL </strong> <%= link_to user_path(@user), user_path(@user) %>
    </td>
  </tr>
    <% if @user.envios.any? %> 
        <table class ="envios" summary="User envios">
          <%= render @envios %> 
        </table>
        <%= will_paginate @envios %>
    <% end %> 
</table>