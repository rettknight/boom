<!DOCTYPE html>
<html>
<head>
  <title>Envios <%= @title%></title>
  <%= csrf_meta_tags %>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag "application" %> 
<body>
<div class = "container">
<%= render 'layouts/header' %> 
<section class ="round">
	<% flash.each do |key, value| %>
<div class = "flash <%= key %>">
	<%= value %> 
	<% end %>
<%= yield %>
</div>
</section>
<%= render 'layouts/footer'%>
<%= debug(params) if Rails.env.development? %> 
</div> 
</body>
</html>
